{
  "name": "Progress AI",
  "version": "1.0",
  "manifest_version": 3,
  "key": "",
  "background": {
    "service_worker": "sw.js"
  },
 "host_permissions": [
    "http://127.0.0.1:5000/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "icons": {
  "128": "logo.png"
  },
  "permissions": ["activeTab","history","readingList"]
}

